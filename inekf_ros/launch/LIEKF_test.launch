<launch>

  <!-- Create data publisher-->
  <param name="/use_sim_time" value="true" />
  <node pkg="rosbag" type="play" name="rosbagplay" args="$(find localization)/src/out_2.bag --clock" required="true"/>

  <!-- Run Invariant EKF -->
  <node name="inekf" pkg="inekf_ros" type="inekf_node" output="screen">
    <!-- <rosparam file="$(find inekf_ros)/config/settings.yaml" command="load" /> -->
    <param name="imu_topic" value="/sensors/an_device/Imu"/>
    <param name="gps_topic" value="/sensors/an_device/NatSatFix"/>
    <param name="map_frame_id" value="map"/>
    <param name="base_frame_id" value="base_link"/>
    <param name="publish_rate" value="60"/>
    <param name="pose_topic" value="/wamv/robot_localization/odometry/filtered"/>
    <param name="state_topic" value="/state"/>
    <param name="publish_visualization_markers" value="false"/>
    <!-- <param name="visualization_markers_topic" value=""/> -->
    <param name="enable_landmarks" value="false"/>
    <!-- <param name="landmarks_topic" value=""/> -->
    <param name="enable_kinematics" value="false"/>
    <!-- <param name="kinematics_topic" value=""/> -->
    <!-- <param name="contact_topic" value=""/> -->
    <rosparam file="$(find inekf_ros)/config/noise.yaml" command="load" />
    <rosparam file="$(find inekf_ros)/config/prior.yaml" command="load" />
  </node>

  <!-- Recording localization data to files in localization pkg -->
  <node name = "listener_xyz" pkg = "localization" type = "listener_xyz"/>
  <!-- <node name = "listener_gps" pkg = "localization" type = "listener_gps"/> -->

  <!-- Rviz -->
  <!-- <node type="rviz" name="rviz" pkg="rviz" args="-d $(find inekf_ros)/rviz/config.rviz" /> -->
  <!-- <node type="rviz" name="rviz" pkg="rviz" args="-d /mnt/c/Users/Ross\ Hartley/Documents/GitHub/personal/catkin_ws/src/invariant-ekf_ros/inekf_ros/rviz/config.rviz" /> -->

</launch>